---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Eclipse CopyTo</title>
 <link href="http://pke.github.com/CopyTo/atom.xml" rel="self"/>
 <link href="http://pke.github.com/CopyTo/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://pke.github.com/CopyTo/</id>
 <author>
   <name>Philipp Kursawe</name>
   <email>phil.kursawe@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://pke.github.com/CopyTo/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://pke.github.com/CopyTo/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>